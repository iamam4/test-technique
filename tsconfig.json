{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es6"],
      "jsx": "react",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "assets/react/controllers/**/*",
      "assets/react/types/**/*"
    ]
  }
  